<Alloy>
	<Collection src="prop" />
    <Window id="detail" title="Detail">
<!-- 		<Button title="Back" onClick="reload" top="50dp" />  -->
		<TableView dataCollection="prop" dataFilter="filterFunction" >
		    <TableViewRow layout="vertical">
		        <ImageView image="{imageurl}" top="20dp" />
		        <Label text="{name}" bottom="20dp" /> 
				<TextArea value="Estate: {estate}"/>
				<TextArea value="Area: {garea}"/>
				<TextArea value="Rent: {rent}"/>
				<TextArea value="Number of bedrooms: {bedrooms}"/>
				<TextArea value="Number of tenant: {tenants}"/>
				<TextArea value="Create at: {createdAt}"/>
				<TextArea value="Update at: {updatedAt}"/>
				<Label text="Address\n" estate="{name}" onClick="mapClick"/>
		    </TableViewRow>
		</TableView>
    </Window>
</Alloy>