	<div class="container-fluid">
	<div class="row">
		<div class="col-md-12">
		<% if(req.session.usrname == null){ %>
            <ul class="nav nav-tabs">
                <li class="active">
                    <a href="http://localhost:1337/prop/index">Home</a>
                </li>
                <li>
                    <a href="http://localhost:1337/prop/search">Search</a>
                </li>
                <li>
                    <a href="http://localhost:1337/usr/login">Login</a>
                </li>
            </ul>
        <%} else {%>
            <ul class="nav nav-tabs">
                <li class="active">
                    <a href="http://localhost:1337/prop/index">Home</a>
                </li>
                <li>
                    <a href="http://localhost:1337/prop/search">Search</a>
                </li>
                <li>
                    <a href="http://localhost:1337/prop/create">Create</a>
                </li>
                <li>
                    <a href="http://localhost:1337/prop/admin">Admin</a>
                </li>
                <li>
                    <a href="http://localhost:1337/usr/logout">Logout</a>
                </li>
                <li class="disabled">
                    Hi, <%=req.session.usrname%>
                </li>
            </ul>
        <% } %>
			<div class="jumbotron">
				<h2>
					Property Rental System
				</h2>
				<p>
					Property Rental Management System is a web-based system to facilitate flatshare and finding roommates.
				</p>
			</div>
		</div>
		<div>
			<% usr.interests.forEach (function(model) { %>
			<table class="table">
				<tr>
					<td width="400px"><%=model.name%></td>
					<td><a href="http://localhost:1337/prop/interestMember/<%=model.id%>">Interested Member</a></td>
                    <% if(req.session.usrname == 'admin'){ %>
					<td><a href="http://localhost:1337/prop/update/<%=model.id%>">Edit</a></td>
					<td><a href="http://localhost:1337/prop/delete/<%=model.id%>">Delete</a></td>
                    <% } else { %>
                    <td><a href='/usr/removeInterest/<%=model.id%>'>Remove</a></td>
                    <% } %>
				</tr>
			</table>
			<% }); %>
            <div>
                <button type="button" class="btn btn-default"><a href="http://localhost:1337/prop/admin">Reload to see changed interested property!</a></button>
            </div>
		</div>
	</div>
	</div>


